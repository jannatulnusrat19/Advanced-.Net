@model Lab3.Models.Database.Account
@{
    ViewBag.Title = "Add";
}

<h2>Edit</h2>

<form action="" method="post">
    <label for="name">Name: </label>
    <input type="text" name="Name" value="@Model.Name" class="form-control" />
    <label for="Email">Email: </label>
    <input type="text" name="Email" value="@Model.Email" class="form-control" />
    <label for="Date_of_Birth">Date of Birth: </label>
    <input type="text" name="Date_of_Birth" value="@Model.Date_of_Birth" class="form-control" />
    <label for="Address">Address: </label>
    <input type="text" name="Address" value="@Model.Address" class="form-control" />
    <label for="Mobile_Number">Mobile Number: </label>
    <input type="text" name="Mobile_Number" value="@Model.Mobile_Number" class="form-control" />
    <label for="id">ID: </label>
    <input type="text" name="ID" value="@Model.ID" class="form-control" />
    <input type="submit" value="Update" class="btn btn-success" />


</form>
